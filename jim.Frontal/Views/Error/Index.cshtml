@model  jim.Frontal.infraestructura.Comun.AppError


<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    
    <div class="container">
        <div class="row">
            <div class="alert-danger" role="alert">Ha ocurrido un error contacte con el administrador. identificador: @Model.Id</div>
            <div class="alert-warning"  role="alert">@Model.MessageError</div>
            <div class="alert-info"  role="alert">@Model.Trace</div>
        </div>
        
    </div>

    

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @*@Scripts.Render("~/budles/moment");*@
    @Scripts.Render("~/bundles/app");

   

    <script>
        $().ready(function () {

            App.Comun.init();
        });
    </script>
</body>
</html>

